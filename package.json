{
    "name": "components",
    "private": true,
    "scripts": {
        "prestart": "scripts/generatePackageData.js && scripts/createSandbox.js && scripts/buildPackages.js",
        "start": "npm-run-all --parallel generatePackageData:watch package:watch start:server",
        "start:server": "scripts/server.js",
        "generatePackageData:watch": "scripts/generatePackageData.js --watch",
        "build": "npm run prestart && scripts/build.js",
        "open": "scripts/open.js",
        "package:setup": "scripts/setupPackages.js",
        "package:add": "scripts/addPackage.js",
        "package:build": "scripts/buildPackages.js",
        "package:watch": "scripts/watchPackages.js",
        "package:build:watch": "scripts/buildPackages.js && scripts/watchPackages.js",
        "package:examples": "scripts/organizeExamples.js",
        "package:doc": "scripts/generateDoc.js",
        "lint": "scripts/lint.js",
        "test": "scripts/test.js --watch",
        "format": "scripts/format.js",
        "check": "scripts/lint.js && scripts/test.js",
        "release": "scripts/releaseCheck.js && scripts/buildPackages.js && scripts/lint.js -- -p && scripts/test.js -- -p && lerna publish --npm-client=npm --registry=https://registry.npmjs.org/",
        "deploy": "shipit production deploy",
        "postcheckout": "scripts/bootstrap.js && scripts/buildPackages.js",
        "postmerge": "scripts/bootstrap.js && scripts/buildPackages.js",
        "precommit": "scripts/format.js -s && scripts/lint.js -s && scripts/test.js -s"
    },
    "devDependencies": {
        "@babel/cli": "7.1.5",
        "@babel/core": "7.1.6",
        "autoprefixer": "9.3.1",
        "babel-core": "7.0.0-bridge.0",
        "babel-eslint": "10.0.1",
        "babel-jest": "23.6.0",
        "babel-loader": "8.0.4",
        "browser-sync": "2.18.12",
        "chokidar": "1.7.0",
        "colors": "1.1.2",
        "connect-history-api-fallback": "1.3.0",
        "cross-spawn": "5.1.0",
        "css-loader": "1.0.1",
        "escape-html": "1.0.3",
        "eslint": "5.9.0",
        "eslint-config-airbnb": "17.1.0",
        "eslint-config-prettier": "3.3.0",
        "eslint-import-resolver-webpack": "0.10.1",
        "eslint-plugin-import": "2.14.0",
        "eslint-plugin-jest": "22.0.0",
        "eslint-plugin-jsx-a11y": "6.1.2",
        "eslint-plugin-react": "7.11.1",
        "file-loader": "2.0.0",
        "fs-extra": "5.0.0",
        "html-webpack-plugin": "3.2.0",
        "husky": "0.13.3",
        "identity-obj-proxy": "3.0.0",
        "inquirer": "4.0.1",
        "jest": "23.6.0",
        "lerna": "2.8.0",
        "node-sass": "4.7.2",
        "npm-run-all": "4.1.1",
        "postcss-loader": "3.0.0",
        "prettier": "1.8.2",
        "react-docgen": "2.19.0",
        "recursive-readdir": "2.2.2",
        "rimraf": "2.6.2",
        "sass-loader": "7.1.0",
        "shipit-cli": "4.1.1",
        "shipit-deploy": "4.1.1",
        "shipit-utils": "1.4.0",
        "style-loader": "0.23.1",
        "uglifyjs-webpack-plugin": "2.0.1",
        "url-loader": "1.1.2",
        "webpack": "4.25.1",
        "webpack-dev-server": "3.1.10"
    },
    "workspaces": [
        "packages/*"
    ],
    "peerDependencies": {
        "@ieremeev/table": "*",
        "babel-preset-ieremeev": "*",
        "eslint-config-ieremeev": "*"
    },
    "dependencies": {
        "@babel/polyfill": "7.0.0",
        "express": "^4.16.2",
        "lodash": "^4.17.10",
        "prismjs": "^1.8.3",
        "prop-types": "^15.6.0",
        "react": "^16.0.0",
        "react-dnd": "^2.1.4",
        "react-dnd-html5-backend": "^2.1.2",
        "react-dom": "^16.0.0",
        "react-router-dom": "^4.2.2",
        "react-sizeme": "^2.3.6",
        "store": "^2.0.12"
    },
    "babel": {
        "presets": [
            "ieremeev"
        ]
    }
